<% if @videos %>
<div class="container">
	<div class="row">
		<% @videos.each do |video| %>
			<div class="col-md-4 col-xs-6">
				<div class="panel text-center">
					<div class="panel-header text-center">
						<%= video.title %>
					</div>
					<div class="panel-body">
						<iframe id="ytplayer" type="text/html" width="320" height="240" src="http://www.youtube.com/embed/<%= video.link %>?autoplay=0&origin=http://www.google.com" frameborder="0"></iframe>
					</div>
				</div>
			</div>
		<% end %>
	</div>
</div>

<% else %>
No videos here fool!
<% end %>